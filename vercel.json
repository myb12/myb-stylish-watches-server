{
  "version": 2,
  "builds": [{
    "src": "./index.js",
    "use": "@vercel/node"
  }],
"routes": [
    {
      "src": "/api/(.*)",
      "dest": "/"
    }
  ],
   "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
